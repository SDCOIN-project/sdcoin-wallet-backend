{
  "name": "sdcoin-wallet",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "$(npm bin)/migrate-mongo up --file config/migrate-mongo-config.js",
    "create-migration": "$(npm bin)/migrate-mongo create --file config/migrate-mongo-config.js",
    "audit": "audit-ci --config audit-ci.json"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@nestjs/common": "^6.6.7",
    "@nestjs/core": "^6.6.7",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-socket.io": "^6.10.2",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/websockets": "^6.10.2",
    "@sentry/node": "^5.7.1",
    "audit-ci": "^2.3.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bignumber.js": "^9.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.14.0",
    "config": "^3.1.0",
    "crypto-random-string": "^3.0.1",
    "cryptoapi-lib": "0.0.1-rc.2",
    "ethereumjs-tx": "^2.1.1",
    "firebase-admin": "^8.2.0",
    "joi": "^14.3.1",
    "joi-objectid": "^2.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.5.11",
    "multer": "^1.4.2",
    "nest-schedule": "^0.6.3",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "qs": "^6.9.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "secure-pin": "^1.0.14",
    "socket.io": "^2.3.0",
    "swagger-ui-express": "^4.0.5",
    "twilio": "^3.31.1",
    "web3": "1.2.2",
    "web3-utils": "^1.2.4",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.18",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^10.12.18",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^24.9.0",
    "migrate-mongo": "^6.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
